<?xml version="1.0"?>
<includes>
	<include name="_ListSecondary">
		<control type="group">
			<include>FadeDialog</include>
			<include>rightSideSlide</include>
			<visible>Container.content(folders) | Container.content(folder)</visible>
			<control type="group" id="777">
				<control type="fixedlist" id="40">
					<visible>Container.content(folders) | Container.content(folder)</visible>
					<viewtype label="ListSecondary">list</viewtype>
					<posy>50</posy>
					<posx>100</posx>
					<width>1100</width>
					<height>620</height>
					<onright>-</onright>
					<onleft>1300</onleft>
					<onup>40</onup>
					<ondown>40</ondown>
					<focusposition>1</focusposition>
					<wraparound>true</wraparound>
					<scrolltime>200</scrolltime>
					<pagecontrol>61</pagecontrol>
					<itemlayout height="45" width="1000">
						<control type="label">
							<posx>60</posx>
							<width>1000</width>
							<height>42</height>
							<align>left</align>
							<aligny>center</aligny>
							<label>$INFO[ListItem.Label]</label>
							<font>Condensed-28</font>
							<include>White2</include>
						</control>
					</itemlayout>
					<focusedlayout height="85" width="1000">
						<control type="label">
							<posx>80</posx>
							<width>1000</width>
							<height>80</height>
							<align>left</align>
							<aligny>center</aligny>
							<label>$INFO[ListItem.Label]</label>
							<font>CondensedBold-48</font>
							<scroll>false</scroll>
							<textcolor>orangeLight</textcolor>
							<shadowcolor>FF222222</shadowcolor>
						</control>
					</focusedlayout>
				</control>
			</control>
		</control>
	</include>
	<include name="ListSecondary">
		<control type="group">
			<include>FadeDialog</include>
			<include>rightSideSlide</include>
			<visible>Container.content(folders) | Container.content(folder)</visible>
			<control type="group" id="777">
				<control type="list" id="40">
					<visible>Container.content(folders) | Container.content(folder)</visible>
					<viewtype label="ListSecondary">list</viewtype>
					<posy>30</posy>
					<posx>100</posx>
					<width>1200</width>
					<height>655</height>
					<onright>-</onright>
					<onleft>1300</onleft>
					<onup>40</onup>
					<ondown>40</ondown>
					<focusposition>0</focusposition>
					<wraparound>true</wraparound>
					<scrolltime>200</scrolltime>
					<pagecontrol>61</pagecontrol>
					<itemlayout height="50">
						<control type="image">
							<posx>0</posx>
							<posy>0</posy>
							<width>1150</width>
							<height>50</height>
							<texture border="5">border-bottom-grey.png</texture>
						</control>
								<control type="image">
									<posx>20</posx>
									<posy>4</posy>
									<width>40</width>
									<height>40</height>
									<texture>icon-folder.png</texture>
									<aspectratio>keep</aspectratio>
									<animation effect="fade" start="0" end="20" time="0" condition="true">Conditional</animation>
								</control>
						<control type="label">
							<width>1150</width>
							<height>50</height>
							<info>ListItem.Label</info>
							<include>White2</include>
							<textoffsetx>80</textoffsetx>
						</control>
					</itemlayout>
					<focusedlayout height="50">
						<control type="image">
							<posx>0</posx>
							<posy>0</posy>
							<width>1150</width>
							<height>50</height>
							<texture border="5">black-bottom-grey.png</texture>
						</control>
								<control type="image">
									<posx>20</posx>
									<posy>4</posy>
									<width>40</width>
									<height>40</height>
									<texture>icon-folder.png</texture>
									<aspectratio>keep</aspectratio>
									<animation effect="fade" start="0" end="80" time="0" condition="true">Conditional</animation>
								</control>
						<control type="label">
							<width>1150</width>
							<height>50</height>
							<info>ListItem.Label</info>
							<textcolor>Focused</textcolor>
							<textoffsetx>80</textoffsetx>
						</control>
					</focusedlayout>
				</control>
			</control>
		</control>
	</include>
</includes>
